    "@sentry/nextjs": "^10.37.0",
    "@stripe/stripe-js": "^8.7.0",
    "@supabase/supabase-js": "^2.93.2",
    "@tanstack/react-query": "^5.90.20",
    "@vercel/analytics": "^1.6.1",
